<% provide(:title, '従業員の追加') %>
<% provide(:subtitle, '追加する従業員のメールアドレスを入力してください。') %>

<%= form_with url: send_email_branch_path(@branch), local: true do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email, class: 'form-control' %>
  
  <%= f.submit "従業員を登録する", class: "btn btn-primary" %>
<% end %>