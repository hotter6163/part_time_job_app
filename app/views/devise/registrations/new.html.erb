<h2>ユーザー登録</h2>

<%= form_with(model: resource, url: user_registration_path, scope: :user, local: true) do |f| %>
  <%= render "shared/new_user_registration_form", form: f %>
  
  <% if !!params[:branch_id] %>
    <%= hidden_field_tag :branch_id, params[:branch_id] %>
  <% end %>
  
  <div class="actions">
    <%= f.submit "Sign up", class: "btn btn-primary" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
